@model IEnumerable<WebAppCM.Models.CadastralObject>
@{
    ViewBag.Title = "Список Кадастровых обьектов";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <script src="JS/jquery-1.11.0.min.js"></script>
    <script src="/Content/JS/scrypts.js"></script>
</head>
<body>
    <div>
        <h3>Список Кадастровыx обьектов</h3>
        <table class="text-center table-bordered">
            <tr>
                <td style="width: 2%;"><p>№</p></td>
                <td style="width: 10%;"><p>Тип</p></td>
                <td style="width: 8%;"><p>Номер</p></td>
                <td style="width: 10%;"><p>Дата постановки</p></td>
                <td style="width: 10%;"><p>Законный статус</p></td>
                <td style="width: 12%;"><p>Адресс</p></td>
                <td style="width: 8%;"><p>Площадь</p></td>
                <td style="width: 10%;"><p>Стоимотсть</p></td>
                <td style="width: 10%;"><p></p></td>
                <td style="width: 6%;"><p></p></td>
            </tr>
            @foreach (var m in Model)
            {
                <tr>
                    <td><p>@Html.DisplayFor(model => m.Id)</p></td>
                    <td><p>@Html.DisplayFor(model => m.type)</p></td>
                    <td><p>@Html.DisplayFor(model => m.cadastralNumber)</p></td>
                    <td><p>@Html.DisplayFor(model => m.dateOfEntry)</p></td>
                    <td><p>@Html.DisplayFor(model => m.legalStatus)</p></td>
                    <td><p>@Html.DisplayFor(model => m.address)</p></td>
                    <td><p>@Html.DisplayFor(model => m.square)</p></td>
                    <td><p>@Html.DisplayFor(model => m.cost)</p></td>
                    <td><p><a href="/CO/COEdit/@m.Id">Редактировать</a></p></td>
                    <td><p><a href="/CO/CODelete/@m.Id">Удалить</a></p></td>
                </tr>
            }
        </table>
        <br />
        <ul class="btn btn-default btn-lg">
            <p><a href="/CO/COAdd">Создать  новый Кадастровый обьект</a></p>
        </ul>
    </div>
</body>
</html>